program {
  Sacar(Verde)
  Mover(Norte)
  Sacar(Verde)
  Mover(Norte)
  Sacar(Verde)
  Mover(Este)
  Sacar(Verde)
  Mover(Este)
  Sacar(Verde)
  Mover(Sur)
  Sacar(Verde)
  Mover(Sur)
  Sacar(Verde)
  Mover(Oeste)
  Sacar(Verde)
  Mover(Oeste)
}
